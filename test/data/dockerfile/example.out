ARG example=''
ARG example_foo='a'

ARG _alpine_version='3.9.4'

FROM alpine AS example
ARG example
ARG example_bar='b'
RUN if [[ -n "${example}" ]]; then \
    touch /root/example \
  ; fi

FROM alpine:"${_alpine_version}"

ARG example
ARG example_baz='c'
COPY --from=example /root/example* /var/empty /usr/local/bin/
RUN if [[ -n "${example}" ]]; then \
    ls \
  ; fi

WORKDIR /workdir
